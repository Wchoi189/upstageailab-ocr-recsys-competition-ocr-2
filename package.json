{
  "name": "upstage-console-workspace",
  "private": true,
  "workspaces": [
    "apps/frontend",
    "apps/playground-console",
    "packages/console-shared",
    "apps/agentqms-dashboard/frontend",
    "apps/ocr_inference_console"
  ],
  "scripts": {
    "dev:spa": "npm run dev --workspace frontend-app",
    "dev:console": "npm run dev --workspace apps/playground-console",
