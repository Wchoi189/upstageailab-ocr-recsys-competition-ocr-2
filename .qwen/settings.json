{
  "general": {
    "checkpointing": {
      "enabled": true
    }
  },
  "ui": {
    "showMemoryUsage": true
  },
  "telemetry": {
    "enabled": false
  },
  "tools": {
    "approvalMode": "yolo"
  },
  "workspace": {
    "includeDirectories": [
      "/workspaces/upstageailab-ocr-recsys-competition-ocr-2"
    ],
    "excludePatterns": [
      "**/node_modules/**",
      "**/.git/**",
      "**/.copilot/**",
      "**/.cursor/**",
      "**/.github/**",
      "**/*.pyc",
      "**/__pycache__/**",
      "**/logs/**",
      "**/outputs/**",
      "**/data/**",
      "**/multirun/**",
      "**/results/**",
      "**/wandb/**",
      "**/docs/_deprecated/**",
      "**/DEPRECATED/**",
      "**/_archive/**",
      "**/_*/**",
      "**/raw_metrics*.json",
      "**/*.jsonl",
      "**/multirun.yaml",
      "**/test_runs/**"
    ]
  },
  "agentqms": {
    "enabled": true,
    "instructions": "/workspaces/upstageailab-ocr-recsys-competition-ocr-2/AgentQMS/knowledge/agent/system.md",
    "architecture": "/workspaces/upstageailab-ocr-recsys-competition-ocr-2/.agentqms/state/architecture.yaml",
    "artifactsPath": "/workspaces/upstageailab-ocr-recsys-competition-ocr-2/docs/artifacts",
    "validation": {
      "enabled": true,
      "ignorePaths": [
        "docs/*.md",
        "docs/assets/**",
        "docs/archive/**",
        ".copilot/**",
        ".cursor/**",
        ".github/**"
      ]
    }
  },
  "context": {
    "fileFiltering": {
      "respectGitIgnore": true,
      "respectGeminiIgnore": true,
      "includePatterns": [
        "**/*.py",
        "**/*.md",
        "**/requirements.txt",
        "**/pyproject.toml",
        "docs/artifacts/**"
      ],
      "excludePatterns": [

      ]
    },
    "includeDirectories": [],
    "loadMemoryFromIncludeDirectories": false,
    "discoveryMaxDirs": 15,
    "importFormat": "tree"
  },
  "model": {
    "name": "qwen/qwen3-coder"
  },
  "$version": 2
}
