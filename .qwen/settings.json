{
  "general": {
    "checkpointing": {
      "enabled": true
    }
  },
  "ui": {
    "showMemoryUsage": true
  },
  "telemetry": {
    "enabled": false
  },
  "tools": {
    "approvalMode": "yolo"
  },
  "context": {
    "fileFiltering": {
      "respectGitIgnore": true,
      "respectQwenIgnore": true,
      "context_aware_loading": true,
      "includePatterns": [
        "**/pyproject.toml",
        "**/AGENTS.yaml",
        "**/AGENTS.md",
        "**/README.md",
        "**/.qwen/QWEN.md",
        "**/AgentQMS/**/*",
        "**/ocr/**/*",
        "**/configs/**/*",
        "**/project_compass/**/*",
        "**/scripts/**/*",
        "**/runners/**/*",
        "**/bin/**/*",
        "**/Makefile",
        "**/package.json",
        "**/pyproject.toml",
        "**/experiment_manager/**/*",
        "**/agent-debug-toolkit/**/*",
        "**/airflow-batch-processor/**/*",
        "**/analysis/**/*",
        "**/apps/**/*",
        "**/packages/**/*",
        "**/tests/**/*"
      ],
      "excludePatterns": [
        "**/node_modules/**",
        "**/.git/**",
        "**/logs/**",
        "**/outputs/**",
        "**/data/**",
        "**/__pycache__/**",
        "**/*.pyc",
        "**/.pytest_cache/**",
        "**/dist/**",
        "**/build/**",
        "**/venv/**",
        "**/.venv/**",
        "**/archive/**",
        "**/_*/**",
        "**/docs/assets/**",
        "**/docs/generated/**",
        "**/experiment_manager/experiments/**/outputs/**",
        "**/experiment_manager/experiments/**/checkpoints/**",
        "**/experiment_manager/experiments/**/__pycache__/**",
        "**/AgentQMS/standards/tier3-agents/claude/**/*"
      ]
    },
    "includeDirectories": [
      "airflow-batch-processor"
    ],
    "loadMemoryFromIncludeDirectories": false,
    "discoveryMaxDirs": 15,
    "importFormat": "tree"
  },
  "model": {
    "name": "qwen/qwen3-coder"
  },
  "$version": 2,
  "mcpServers": {
    "unified_project": {
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/workspaces/upstageailab-ocr-recsys-competition-ocr-2",
        "python",
        "scripts/mcp/unified_server.py"
      ]
    },
    "perplexity": {
      "command": "npx",
      "args": [
        "-y",
        "perplexity-mcp"
      ],
      "env": {
        "PERPLEXITY_API_KEY": "${PERPLEXITY_API_KEY}"
      }
    }
  }
}
