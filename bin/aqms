#!/bin/bash
# AgentQMS Unified CLI Entrypoint
# Single source of truth for AgentQMS commands

set -e

SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
PROJECT_ROOT="$(cd "$SCRIPT_DIR/.." && pwd)"

# Ensure we're using project venv
export PYTHONPATH="$PROJECT_ROOT:$PYTHONPATH"

# Route to Python CLI implementation (the actual tool)
exec uv run python "$PROJECT_ROOT/AgentQMS/bin/qms" "$@"
